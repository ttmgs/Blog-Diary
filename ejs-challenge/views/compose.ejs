<%- include("partials/header") -%>


<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="mb-3">
    <label for="exampleFormControlInput1" class="form-label">Title</label>
    <input type="text" class="form-control" name="composeTitle" placeholder="">
  </div>
  <div class="mb-3">
    <label for="exampleFormControlTextarea1" class="form-label">Post</label>
    <textarea class="form-control" name="composePost" rows="3"></textarea>
  </div>
  <br>
  <button type="submit" class="btn btn-primary">Publish</button></form>




<%- include("partials/footer") -%>
